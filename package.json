{
  "name": "tailwind-ink",
  "version": "0.0.1-alpha-0",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "tshy"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "ink": "^4.4.1",
    "tshy": "^1.7.0"
  },
  "tshy": {
    "dialects": [
      "esm"
    ],
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  }
}
